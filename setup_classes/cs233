#!/bin/bash
echo "Installing Packages..."
get_packages "cs233" "gtkwave-3.3.40-1 iverilog-0.9.6-1"
if [ "$?" -ne "0" ]; then
	return 1
fi

echo "Checking out your cs233 folder..." >&2
if [ ! -d "$HOME/svn/cs233" ]; then
	subversion_checkout "cs398" "cs233" "0" >/dev/null
	if [ "$?" -ne "0" ]; then
		return $?
	fi
fi
echo "Checked out the folder into ~/svn/cs233" >&2
return 0
